<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
	android:background="@color/listView_rowBgColor" >
  	<sg.com.innosys.wms.UI.Common.ListViewItems
		android:id="@+id/row_unbalanceput_PalletID"		
		android:layout_height="wrap_content"
		android:layout_width="@dimen/listPalletID_width"
		android:paddingTop="@dimen/listViewRowpaddingTop"
		android:paddingBottom="@dimen/listViewRowpaddingBottom"
		android:textSize="@dimen/listViewRowTextSize_normal"
		android:textColor="@color/listView_rowTextColor"/>
	<sg.com.innosys.wms.UI.Common.ListViewItems
		android:id="@+id/row_unbalanceput_Zone"
		android:layout_height="wrap_content"
		android:layout_width="@dimen/listZone_width"
		android:paddingTop="@dimen/listViewRowpaddingTop"
		android:paddingBottom="@dimen/listViewRowpaddingBottom"
		android:textSize="@dimen/listViewRowTextSize_normal"
		android:textColor="@color/listView_rowTextColor"/>		
	<sg.com.innosys.wms.UI.Common.ListViewItems
		android:id="@+id/row_unbalanceput_Row"
		android:layout_height="wrap_content"
		android:layout_width="@dimen/listRow_width"
		android:paddingTop="@dimen/listViewRowpaddingTop"
		android:paddingBottom="@dimen/listViewRowpaddingBottom"
		android:textSize="@dimen/listViewRowTextSize_normal"
		android:textColor="@color/listView_rowTextColor"/>		
	<sg.com.innosys.wms.UI.Common.ListViewItems
		android:id="@+id/row_unbalanceput_Column"
		android:layout_height="wrap_content"
		android:layout_width="@dimen/listColumn_width"
		android:paddingTop="@dimen/listViewRowpaddingTop"
		android:paddingBottom="@dimen/listViewRowpaddingBottom"
		android:textSize="@dimen/listViewRowTextSize_normal"
		android:textColor="@color/listView_rowTextColor"/>		
	<sg.com.innosys.wms.UI.Common.ListViewItems
		android:id="@+id/row_unbalanceput_Tier"
		android:layout_height="wrap_content"
		android:layout_width="@dimen/listTier_width"
		android:textSize="@dimen/listViewRowTextSize_normal"
		android:textColor="@color/listView_rowTextColor"/>	
	<sg.com.innosys.wms.UI.Common.ListViewItems
		android:id="@+id/row_unbalanceput_ActQty"
		android:layout_height="wrap_content"
		android:layout_width="@dimen/listQty_width"
		android:paddingTop="@dimen/listViewRowpaddingTop"
		android:paddingBottom="@dimen/listViewRowpaddingBottom"
		android:textSize="@dimen/listViewRowTextSize_normal"
		android:textColor="@color/listView_rowTextColor"/>		
</LinearLayout>